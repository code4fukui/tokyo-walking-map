      <Placemark>
        <name>コース／Route</name>
        <styleUrl>#line-FF5252-3131-nodesc</styleUrl>
        <LineString>
          <tessellate>1</tessellate>
          <coordinates>
            139.4444324,35.7558942,0
            139.4434226,35.7555568,0
